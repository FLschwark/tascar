  scene [label="scene\n[name]",style="filled",fillcolor="gold"];
  source [label="pointsource\n[name]",style="filled",fillcolor="gold"];
  sound [label="sound\n[file]\n[start] s\n[gain] dB",style="filled",fillcolor="gold"];
